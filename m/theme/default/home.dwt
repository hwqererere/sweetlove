<script>
{literal}
  var div=$(".banner").find("div");
  if(div.length>1){
    $(".banner").attr("_index","0");
    setInterval(function(){
      var nex=$(".banner").attr("_index")-0+1;
      if(nex==div.length){
        nex=0;
      }
      div.eq($(".banner").attr("_index")).css("z-index","999");
      div.eq(nex).css("z-index","99");
      div.eq($(".banner").attr("_index")).animate({opacity:0},1000,function(){
        div.eq($(".banner").attr("_index")).css("z-index","0");
        div.eq($(".banner").attr("_index")).css("opacity","1");
        $(".banner").attr("_index",nex);
      });
    },10000);
  }

  function AddFavorite(title, url) {
 try {
   window.external.addFavorite(url, title);
 }
catch (e) {
   try {
    window.sidebar.addPanel(title, url, "");
  }
   catch (e) {
     alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请进入新网站后使用Ctrl+D进行添加");
   }
 }
}
{/literal}
