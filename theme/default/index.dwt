<script>
{literal}


var time="";

function activeon(mid){
  var div=$(".banner").find("div");
  if(div.length>1){
    time=setInterval(function(){
      var nex=$(".banner").attr("_index")-0+1;
      if(nex==div.length){
        nex=0;
      }
      banner(nex);
    },mid);

  }
  
}

$(".bannerpoint li").hover(function(){
    var nex=$(this).index();
    banner(nex,1000);
    clearTimeout(time);
},function(){
    activeon(10000);
});



activeon(10000);

function banner(nex){ 
      var div=$(".banner").find("div");     
      div.eq($(".banner").attr("_index")).css("z-index","999");
      div.eq(nex).css("z-index","99");
      div.eq($(".banner").attr("_index")).animate({opacity:0},100,function(){
        div.eq($(".banner").attr("_index")).css("z-index","0");
        div.eq($(".banner").attr("_index")).css("opacity","1");
        $(".banner").attr("_index",nex);
        $(".bannerpoint li").removeClass("sel");
        $(".bannerpoint li").eq(nex).addClass("sel");
        
      });
    }


